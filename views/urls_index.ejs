<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>TinyApp - My URLs</title>
</head>
<body>
  <h3>My URLs</h3>
  <table>
    <thead>
      <tr>
        <th>Short URL ID</th>
        <th>Long URL</th>
        <th>Edit</th>
        <th>Delete</th>
      </tr>
    </thead>
    <tbody>
      <% for (let id in urls) { %>
        <tr>
          <td><%= id %></td>
          <td><%= urls[id] %></td>
          <td><a href="/urls/<%= id %>">Edit</a></td>
          <td>
            <form method="POST" action="/urls/<%= id %>/delete">
              <button type="submit">Delete</button>
            </form>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>
</body>
</html>

